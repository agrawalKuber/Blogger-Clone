<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle"></label>
    <input class="form-control" type="text" name="postTitle" id="postTitle" />
    <label for="postBody"></label>
    <textarea
      class="form-control"
      name="postBody"
      cols="5"
      rows="5"
      id="postBody"
    ></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer"); -%>
